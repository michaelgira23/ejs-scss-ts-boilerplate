{
  "name": "ejs-scss-ts-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating a simple website using EJS, Sass, and Typescript.",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && ts-node server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint --project tsconfig.json --config tslint.json 'src/ts/**/*.ts'",
    "build": "webpack --config webpack.config.ts",
    "build:watch": "webpack --config webpack.config.ts --watch",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelgira23/ejs-scss-ts-boilerplate.git"
  },
  "keywords": [
